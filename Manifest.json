{
    "name": "Adfree Saavn Experience",
    "description": "Get ad free saavn web experience in free plan.",
    "version": "0.1",
    "permissions": [
    "tabs","<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [
        {
        "matches": [
            "http://*.saavn.com/*"
            ],
        "js": ["jquery.js","content.js"],
        "run_at": "document_end"         
        }
    ], 
    "manifest_version":2
}
